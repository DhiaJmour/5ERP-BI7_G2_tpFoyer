pipeline {
    agent any
    options {
    buildDiscarder(logRotatore(numToKeepStr: '5'))

    }

    stages {
        
        stage('Scan') {
            steps {
                withSonarQubeEnv(installationName: 'sq2' ){
                sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.2155:sonar'
                }
            }
        }
    }

    post {
        success {
            echo 'Pipeline completed successfully!'
        }
        failure {
            echo 'Pipeline failed.'
        }
    }
}

